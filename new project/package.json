{
  "name": "hackathon-platform",
  "version": "1.0.0",
  "description": "Full-stack Hackathon Management Platform",
  "private": true,
  "workspaces": [
    "backend",
    "frontend-nextjs"
  ],
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend-nextjs && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend-nextjs && npm run dev",
    "build:backend": "cd backend && npm install",
    "build:frontend": "cd frontend-nextjs && npm run build",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend-nextjs && npm start"
  },
  "keywords": ["hackathon", "management", "platform"],
  "author": "",
  "license": "ISC"
}
